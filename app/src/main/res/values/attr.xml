<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalNumberPicker">
        <attr name="minValue" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="currentValue" format="integer"/>
        <attr name="mode" format="enum">
            <enum name="production" value="0"/>
            <enum name="currentValue" value="1"/>
            <enum name="temperature" value="2"/>
            <enum name="oxygen" value="3"/>
            <enum name="oceans" value="4"/>
            <enum name="terraforming_rating" value="5"/>
        </attr>
    </declare-styleable>

</resources>